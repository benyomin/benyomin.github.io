@article{semyonov2000,
	Abstract = {The present study addresses the issue of ethnic inequality in homeownership. We start out by addressing the social significance of homeownership as it relates to material wellbeing, sense of security, perceived "stake in the system," and social assimilation. The empirical analysis focuses on immigrants to Israel and examines the extent to which length of residence in the host society, as well as the period of migration, affect ethnic inequality in homeownership. In order to separate the effects of specific period of migration from duration of residence, we pooled data from three household expenditure-surveys carried out between 1975 and 1993. The findings confirmed the positive effect of length of residence in the host society on homeownership, but also revealed unique period effects. These are linked to the political economy of Israel and to the role played by the state in shaping housing opportunities. The findings also revealed substantial ethnic group differences in homeownership. The source of these disparities and their consequences highlight stratification processes that are independent of labor market achievements.},
	Author = {Noah Lewin-Epstein and Moshe Semyonov},
	Date-Added = {2016-12-21 19:34:12 +0000},
	Date-Modified = {2016-12-22 12:52:53 +0000},
	Issn = {00377791},
	Journal = {Social Problems},
	Number = {3},
	Pages = {425-444},
	Publisher = {[Oxford University Press, Society for the Study of Social Problems]},
	Title = {Migration, Ethnicity, and Inequality: Homeownership in Israel},
	Url = {http://www.jstor.org/stable/3097238},
	Volume = {47},
	Year = {2000},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.jstor.org/stable/3097238}}

@article{10.2307/20007592,
	Abstract = {The analysis of 1988-1994 statistical data indicates that the population of Israel simultaneously moves in two opposite directions: while the initial distribution of new immigrants is primarily focused on the areas where jobs are available, the existing population of these areas tends to move outward, to the less populated districts where housing is more available and affordable. The paper thus argues that the heterogeneity of the population is indeed a crucial consideration for the proper modeling of migration behavior. It is also argued that a) the effect of housing construction on the patterns of in-country migration appears to be attenuated, and b) the low attractiveness of peripheral districts of the country to the new immigrants is mainly caused by a lack of jobs rather than by low rates of housing construction or the harsh climatic conditions of these areas.},
	Author = {Boris A. Portnov},
	Date-Added = {2016-12-21 19:34:12 +0000},
	Date-Modified = {2016-12-21 19:34:12 +0000},
	Issn = {09331433, 14321475},
	Journal = {Journal of Population Economics},
	Number = {3},
	Pages = {379-394},
	Publisher = {Springer},
	Title = {The Effect of Housing on Migrations in Israel: 1988-1994},
	Url = {http://www.jstor.org/stable/20007592},
	Volume = {11},
	Year = {1998},
	Bdsk-Url-1 = {http://www.jstor.org/stable/20007592}}

@article{10.2307/490938,
	Abstract = {Based on a case study of immigrant housing projects in Israel, this article focuses on social differentiation among public housing projects, and seeks to expose the mechanism through which such a differentiation is generated. Two sets of factors are suggested as determining the social differentiation among housing projects: (a) endogenous factors associated with the initial conditions, i.e. locational, and social characteristics, (b) exogenous factors which have to do with public housing construction and migration processes occuring in the projects after the initial conditions were already moulded. Through metric and non-metric analysis, the social differentiation among the projects is ascertained, and the relative importance of each factor shaping it is demonstrated.},
	Author = {Shlomo Hasson},
	Date-Added = {2016-12-21 19:34:12 +0000},
	Date-Modified = {2016-12-21 19:34:12 +0000},
	Issn = {04353684, 14680467},
	Journal = {Geografiska Annaler. Series B, Human Geography},
	Number = {2},
	Pages = {95-103},
	Publisher = {[Swedish Society for Anthropology and Geography, Wiley]},
	Title = {Social Differentiation among Public Housing Projects in Israel},
	Url = {http://www.jstor.org/stable/490938},
	Volume = {65},
	Year = {1983},
	Bdsk-Url-1 = {http://www.jstor.org/stable/490938}}

