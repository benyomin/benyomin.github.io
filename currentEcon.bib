@article{cristini2014house,
	Abstract = {This paper undertakes a comparison exercise to disentangle what drives the opposite
findings regarding the effect of house prices on consumption documented in two papers
using the same data set for the UK. On the one hand, Campbell and Cocco (2007) find that
old owners are the most benefited by a house price increase and young renters the least,
confirming the so-called wealth hypothesis. On the other hand, Attanasio, Blow, Hamilton,
and Leicester (2009) find that house prices have the same impact on consumption across
age groups, consistent with the so-called common factor hypothesis. First, we confirm that
the findings in both papers can be reproduced. Second, we rule out a number of potential
reasons related to the basic data construction, and provide evidence that the functional form
(i.e., an Euler equation of consumption vs. a reduced form life-cycle model) and not data
aggregation considerations (household level data vs. synthetic cohort data) may be at the
root of the conflicting results in the two papers. Our findings revive the debate of whether
there is an effect of house prices on consumption.
  },
	Author = {Cristini, Annalisa and Sevilla, Almudena},
	Journal = {Economica},
	Number = {324},
	Pages = {601--625},
	Publisher = {Wiley Online Library},
	Title = {Do House Prices Affect Consumption? A Re-assessment of the Wealth Hypothesis},
	Volume = {81},
	Year = {2014},
	Bdsk-File-1 = {}}

@article{cristini2007house,
	Abstract = {Campbell and Cocco (2007) and Attanasio, Blow, Hamilton, and Leicester (2005a) (CC
and ABHL hereafter) look at the effect of house prices on consumption in the UK and arrive at
two opposite conclusions. CC compares groups of consumers classified by age-homeownership
status and find that old owners are the most benefited by a house price increase and young
renters the least. They interpret these findings as supportive of the wealth hypothesis.
ABHL follow a different route and compare consumers by age status. These authors find
that house prices have the same impact on consumption across age groups, which according
to the authors suffices to discard the wealth channel hypothesis in favor of the common
factor hypothesis. This paper performs a comparison exercise between the two papers to
see what is the driving factor that causes both papers to reach these opposite conclusions.
Special attention is given to the dimension (age or home ownership) that is used by each
paper to test these two hypothesi. We first show that, once ABHL's specification has been
fully modified to replicate that of CC, the wealth hypothesis continues to be challenged
across the age dimension: The consumption of the young and old do not react significantly
different to a house price increase. Results from the comparison across ownership groups
(owners and renters), and across age-ownership status groups are however not robust to the
different specifications used. To the extent that there are differential effects of house prices on
consumption depending on age-home ownership status, the fact that the conclusions depends
on the specification used calls for a more rigorous theoretical discussion on the right empirical
specification that is needed to test the wealth vs. the common factor hypothesis.},
	Author = {Cristini, Annalisa and Sevilla, Almudena},
	Date-Modified = {2016-12-18 18:12:27 +0000},
	Journal = {Mimeo},
	Month = {7},
	Publisher = {University of Bergamo},
	Title = {Do house prices affect consumption and why? A replication and comparison exercise},
	Year = {2007}}

